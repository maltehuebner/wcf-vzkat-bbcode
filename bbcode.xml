<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com http://www.woltlab.com/XSD/maelstrom/bbcode.xsd">
	<import>
		<bbcode name="zeichen">
			<allowedchildren>none</allowedchildren>
			<classname>wcf\system\bbcode\ZeichenBBCode</classname>
			<attributes>
				<attribute name="0">
					<required>1</required>
					<usetext>1</usetext>
					<validationpattern>^(\d{3,4})((\-\d{1,2}\,\d{1,2})|([\.\-]?\d{0,4})([\-]?\d{0,3}))$</validationpattern>
				</attribute>
			</attributes>
		</bbcode>
	</import>
</data>